{
  "name": "Admin",
  "plural": "Users",
  "base": "User",
  "scope": {
    "include": [
      "roles",
      "regions"
    ]
  },
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "admin_id": {
      "type": "string",
      "id": true,
      "required": true,
      "defaultFn": "uuid"
    },
    "new_user": {
      "type": "boolean",
      "required": false,
      "default": true
    },
    "notifications": {
      "type": "string",
      "default": "none"
    },
    "user_notifications": {
      "type": "string",
      "default": "none"
    },
    "description": {
      "type": "string",
      "required": false,
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 280
      }
    }
  },
  "validations": [],
  "relations": {
    "regions": {
      "type": "hasMany",
      "model": "Regions",
      "foreignKey": "admin_id",
      "through": "UsersRegions",
      "keyThrough": "region_id"
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "create"
    },
    {
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW",
      "accessType": "READ"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "checkAccessToken"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "modifyUserNotifications"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW",
      "property": "modifyUserNotifications"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW",
      "property": "editUser"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "fetchUserData"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW",
      "property": "updateSettings"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "updateSettings"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW",
      "property": "fetchUserData"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "createUser"
    }
  ],
  "methods": {}
}