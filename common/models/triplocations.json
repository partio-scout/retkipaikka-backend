{
  "name": "Triplocations",
  "base": "PersistedModel",
  "properties": {
    "location_id": {
      "type": "string",
      "id": true,
      "required": true,
      "defaultFn": "uuid"
    },
    "object_type": {
      "type": "string",
      "required": true
    },
    "location_category": {
      "type": "number",
      "required": true
    },
    "object_name": {
      "type": "string",
      "required": true
    },
    "location_name": {
      "type": "string",
      "required": true
    },
    "location_geo": {
      "type": "GeoPoint",
      "required": true
    },
    "location_description": {
      "type": "string",
      "required": false
    },
    "location_pricing": {
      "type": "string",
      "required": false
    },
    "location_accepted": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "location_owner": {
      "type": "string",
      "required": true
    },
    "location_website": {
      "type": "string",
      "required": false
    },
    "location_phone": {
      "type": "string",
      "required": false
    },
    "location_mail": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "filters": {
      "type": "hasMany",
      "model": "Filters",
      "foreignKey": "locationId",
      "through": "Locationfeatures",
      "keyThrough": "filterId"
    },
    "categories": {
      "type": "belongsTo",
      "model": "Categories",
      "foreignKey": "location_category"
    },
    "images": {
      "type": "hasMany",
      "model": "Images",
      "foreignKey": "locationId"
    }
  },
  "mixins": {
    "SoftDelete": true,
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "acls": [],
  "methods": {}
}