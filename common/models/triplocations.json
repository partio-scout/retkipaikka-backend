{
  "name": "Triplocations",
  "base": "PersistedModel",
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "location_id": {
      "type": "string",
      "id": true,
      "required": true,
      "defaultFn": "uuid"
    },
    "object_type": {
      "type": "string",
      "required": true
    },
    "location_category": {
      "type": "number",
      "required": true
    },
    "location_municipality": {
      "type": "number",
      "required": false
    },
    "location_region": {
      "type": "number",
      "required": true
    },
    "location_name": {
      "type": "string",
      "required": true
    },
    "location_geo": {
      "type": "GeoPoint",
      "required": true
    },
    "location_description": {
      "type": "string",
      "required": false
    },
    "location_pricing": {
      "type": "string",
      "required": false
    },
    "location_accepted": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "location_owner": {
      "type": "string",
      "required": true
    },
    "location_website": {
      "type": "string",
      "required": false
    },
    "location_phone": {
      "type": "string",
      "required": false
    },
    "location_mail": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "filters": {
      "type": "hasMany",
      "model": "Filters",
      "foreignKey": "location_id",
      "through": "Locationfeatures",
      "keyThrough": "filter_id"
    },
    "categories": {
      "type": "belongsTo",
      "model": "Categories",
      "foreignKey": "location_category"
    },
    "municipalities": {
      "type": "belongsTo",
      "model": "Municipalities",
      "foreignKey": "location_municipality"
    },
    "regions": {
      "type": "belongsTo",
      "model": "Regions",
      "foreignKey": "location_region"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "fetchLocations"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "addNewLocation"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "deleteLocation"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "editLocation"
    }
  ],
  "methods": {}
}
